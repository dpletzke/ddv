{
  "name": "dominion-deck-viewer",
  "description": "my ext",
  "version": "0.1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "./icon-16.png",
      "32": "./icon-32.png",
      "48": "./icon-48.png",
      "128": "./icon-128.png"
    }
  },
  "background": {
    "service_worker": "./background.js"
  },
  "permissions": ["storage", "activeTab", "scripting", "tabs"],
  "host_permissions": ["https://dominion.games/"],
  "options_page": "./options.html"
}
