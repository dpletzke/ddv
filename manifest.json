{
  "name": "dominion-deck-viewer",
  "description": "dominon contents deck tracker",
  "version": "0.1.0",
  "manifest_version": 3,
  "action": {
    "default_icon": {
      "16": "./16.png",
      "48": "./48.png",
      "128": "./128.png"
    }
  },
  "background": {
    "service_worker": "./background.js"
  },
  "permissions": ["scripting"],
  "host_permissions": ["https://dominion.games/"]
}
